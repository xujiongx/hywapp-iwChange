{
  "name": "iwChange",
  "outputPath": "dist",
  "releasePath": "release",
  "extId": "cyvxhim7",
  "builder": {
    "name": "@hyext/builder-beyond",
    "config": {
      "webSDKVersion": "1.8.0",
      "webTitle": "虎牙小程序",
      "useCssLoader": true,
      "designWidth": 750,
      "https": true,
      "host": "localhost",
      "port": 18083,
      "enableLint": false,
      "buildConfig": {
        "RN": [
          {
            "entry": "index.js",
            "extType": "app_panel",
            "platform": "app",
            "registerComponent": [
              "iwChange"
            ]
          },
          {
            "entry": "index.js",
            "extType": "app_popup",
            "platform": "app",
            "registerComponent": [
              "iwChange"
            ]
          }
        ],
        "H5": [
          {
            "entry": "index.js",
            "extType": "web_video_com",
            "platform": "web"
          },
          {
            "entry": "index.js",
            "extType": "web_popup",
            "platform": "web"
          },
          {
            "entry": "index.js",
            "extType": "pc_panel",
            "platform": "pc"
          },
          {
            "entry": "index.js",
            "extType": "pc_popup",
            "platform": "pc"
          },
          {
            "entry": "index_streamer.js",
            "extType": "pc_anchor_panel",
            "platform": "pc"
          }
        ]
      }
    }
  }
}